{
	"name": "idna-uts46-hx",
	"description": "A UTS #46 processing algorithm for IDNA2008 strings",
	"version": "4.0.0",
	"private": false,
	"author": {
		"name": "Kai Schwarz",
		"email": "kschwarz@hexonet.net"
	},
	"main": "uts46.js",
	"typings": "index.d.ts",
	"license": "MIT",
	"engines": {
		"node": ">=14.17.0",
		"npm": ">=6.14.13"
	},
	"homepage": "https://github.com/hexonet/idna-uts46",
	"repository": "github:hexonet/idna-uts46",
	"bugs": "https://github.com/hexonet/idna-uts46/issues",
	"maintainers": [
		{
			"name": "Kai Schwarz",
			"email": "kschwarz@hexonet.net"
		}
	],
	"keywords": [
		"hexonet",
		"unicode",
		"idn",
		"idna",
		"domain"
	],
	"nyc": {
		"reporter": [
			"html",
			"text"
		],
		"exclude": [
			"**/*.spec.js",
			"idna-map.js"
		]
	},
	"xo": {
		"prettier": true,
		"globals": [],
		"ignores": [
			"!@(vendor|coverage|.nyc_output|.github|node_modules|pkg|build)/**",
			"!**/*.@(all|min).js"
		]
	},
	"scripts": {
		"test": "npm run test-COVERAGE --silent",
		"test-MOCHA": "./node_modules/.bin/_mocha $(find ./test -type f -name '*.spec.js')",
		"test-COVERAGE": "cross-env NODE_ENV=development nyc npm run test-MOCHA --silent",
		"lint": "xo && npm run lint-python",
		"lint-python": "./scripts/pep8fix.sh"
	},
	"dependencies": {
		"punycode": "^2.1.1"
	},
	"devDependencies": {
		"@hexonet/semantic-release-github-npm-config": "^2.0.4",
		"@semantic-release/exec": "^6.0.2",
		"cross-env": "^7.0.3",
		"gulp": "^4.0.2",
		"gulp-esbuild": "^0.10.3",
		"gulp-prettier": "^4.0.0",
		"gulp-rename": "^2.0.0",
		"gulp-terser": "^2.1.0",
		"mocha": "^10.0.0",
		"nyc": "^15.1.0",
		"prettier-config-xo": "^2.0.0",
		"semantic-release": "^18.0.1",
		"xo": "^0.48.0"
	}
}
